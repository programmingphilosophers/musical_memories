<div id='list'>

  <a href='/memories/create'>Create Item</a>

  <table>
    <thead>
      <th>
        Album
      </th>
      <th>
        Artist
      </th>
      <th>
        Update
      </th>
      <th>
        Destroy
      </th>
    </thead>

    <tbody>
      <% @memories.each do |memory| %>
        <tr>
          <td>
            <span title="<%= memory.whenithappened %>">
              <%= memory.album %>
            </span>
          </td>
          <td>
            <%= memory.artist %>
          </td>
          <td>
            <a href='/memories/update/<%= memory.id %>'>Update</a>
          </td>
          <td>
            <a href='/memories/destroy/<%= memory.id %>'>Destroy</a>
          </td>
        </tr>
      <% end %>
    </tbody>
  </table>

  <p>
    <a href="/logout">Log out.</a>
  </p>
</div>
